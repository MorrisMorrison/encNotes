      </Label>
      <Label layoutX="14.0" layoutY="14.0" text="Settings">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <JFXToggleButton fx:id="btnEncrypt" layoutX="64.0" layoutY="214.0" onAction="#btnEncryptClicked" text="Encrypt Database" />
